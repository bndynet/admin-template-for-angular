{
  "name": "admin-template-for-angular",
  "version": "0.0.0",
  "author": {
    "name": "Bendy Zhang"
  },
  "lisense": "MIT",
  "scripts": {
    "ng": "ng",
    "watch:examples": "watch 'npm run copy:examples' ./src/app/admin/dev/",
    "copy:examples": "ncp ./src/app/admin/dev/ ./src/assets/dev/",
    "copy:readme": "ncp ./README.md ./src/assets/README.md",
    "local": "ng serve --port=9000",
    "start": "run-p copy:examples watch:examples copy:readme local",
    "build": "run-p copy:* && ng build --prod && ts-node ./tools/after-build.ts",
    "deploy": "run-p copy:* && ng build --prod --baseHref=/admin-template-for-angular/ && ts-node ./tools/after-build.ts",
    "test": "ng test",
    "pull:i18n": "git subtree pull --prefix=src/assets/i18n https://github.com/bndynet/i18n.git master --squash",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@bndynet/utils": "^1.0.3",
    "@ctrl/ngx-codemirror": "^4.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "codemirror": "^5.60.0",
    "driver.js": "^0.9.8",
    "keycloak-angular": "^8.0.1",
    "keycloak-js": "^11.0.3",
    "lodash-es": "^4.17.15",
    "moment": "^2.29.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@bndynet/cli": "^1.3.0",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.4.2",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "organize-imports-cli": "^0.8.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "shelljs": "^0.8.4",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "organize-imports-cli",
      "prettier --write"
    ]
  }
}
